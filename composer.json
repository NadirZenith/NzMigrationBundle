{
    "name": "nz/migration-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Nz MigrationBundle with sonata admin",
    "keywords": ["migration", "wordpress", "entity", "orm", "sonata"],
    "homepage": "https://nzlab.es",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Zenith",
            "email": "2cb.md2@gmail.com",
            "homepage": "https://nzlab.es"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/dom-crawler": "~2.1",
        "symfony/css-selector": "~2.1",
        "league/html-to-markdown": "^4.2"
    },
    "require-dev": {
        "nz/wordpress-bundle": "dev-master",
        "sonata-project/formatter-bundle": "~2.2"
    },
    "suggest": {
        "sonata-project/admin-bundle": "~2.3"
    },
    
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/NadirZenith/NzWordpressBundle.git"
        }
       
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nz\\MigrationBundle\\": ""
        }
    }
}
